{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }

           ],
           "no-unused-vars":"off",
           "import/prefer-default-export": "off",
           "trailing-comma": 0

    }
    
}